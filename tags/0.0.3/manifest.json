{
	"name": "Cloud LIU IME",
	"version": "0.0.3",
	"permissions": [	
    	"tabs"
    ],
	"icons": {
		"32": "liuime32.ico",
		"128": "liuime128.ico"
	},
	"browser_action":
	{
		"default_icon": "liuime32.ico",
		"default_title": "LIU:ON"
	},
	"background_page" : "background.html",
	"content_scripts":
	[
		{
			"matches": ["<all_urls>"],
			"css": ["liuime.css"],
			"js": ["liuime.js"],
			"run_at" : "document_end"
		}
	]
}